      "isDeleted": false,
      "boundElements": [],
      "updated": 1738739109346,
      "link": null,
      "locked": false,
      "text": "Здесь значение x делится между двумя областями:\n    - outer\n    - closure",
      "fontSize": 28,
      "fontFamily": 5,
      "textAlign": "left",
      "verticalAlign": "top",
      "containerId": null,
      "originalText": "Здесь значение x делится между двумя областями:\n    - outer\n    - closure",
      "autoResize": true,
      "lineHeight": 1.25
    },
    {
