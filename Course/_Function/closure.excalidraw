      "boundElements": [],
      "updated": 1738739090300,
      "link": null,
      "locked": false,
      "text": "def outer():\n    x = 'python'\n    def inner():\n        print(x)\n    return inner\n",
      "fontSize": 28,
      "fontFamily": 5,
      "textAlign": "left",
      "verticalAlign": "top",
      "containerId": null,
      "originalText": "def outer():\n    x = 'python'\n    def inner():\n        print(x)\n    return inner\n",
      "autoResize": true,
      "lineHeight": 1.25
    },
    {
